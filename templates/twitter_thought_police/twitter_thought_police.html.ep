<script type="text/javascript">
    let currentGlobalTab;
    let currentStateTab;
    % if ($environment eq 'local') {
    let globalTabs = ['global-censored', 'global-users', 'global-docu'];
    % } else {
    let globalTabs = ['global-censored', 'global-docu'];
    % }
    let stateTabs  = ['state-TOTALS', 'state-MODERNA', 'state-PFIZER-BIONTECH', 'state-JANSSEN'];

    $( document ).ready(function() {

        // Loading users banned, sorted, by default, by ban date.
        loadBannedUsers('banned-date');

        // Loading users data sorted, by default, by followers desc.
        % if ($environment eq 'local') {
        loadFollowedUsers('followers');
        % }
    });

    function openStateTab(tabLabel) {
        currentStateTab = tabLabel;
        stateTabs.forEach(setStateTabButton);
        let tab = document.getElementById(tabLabel);
        let tabButton = document.getElementById(tabLabel + '-button');
        tab.style.display = 'block';
        tabButton.style.background = '#262436';
    }

    function setStateTabButton(tabLabel) {
        if (tabLabel != currentTab) {
            let tab = document.getElementById(tabLabel);
            let tabButton = document.getElementById(tabLabel + '-button');
            tab.style.display = 'none';
            tabButton.style.background = '#4f4d66';
        }
    }

    function setTabButton(tabLabel) {
        if (tabLabel != currentTab) {
            let tab = document.getElementById(tabLabel);
            let tabButton = document.getElementById(tabLabel + '-button');
            tab.style.display = 'none';
            tabButton.style.background = '#4f4d66';
        }
    }

    function openGlobalTab(tabLabel) {
        currentGlobalTab = tabLabel;
        globalTabs.forEach(setGlobalTabButton);
        let tab = document.getElementById(tabLabel);
        let tabButton = document.getElementById(tabLabel + '-button');
        tab.style.display = 'block';
        tabButton.style.background = '#262436';
    }

    function setGlobalTabButton(tabLabel) {
        if (tabLabel != currentGlobalTab) {
            let tab = document.getElementById(tabLabel);
            let tabButton = document.getElementById(tabLabel + '-button');
            tab.style.display = 'none';
            tabButton.style.background = '#4f4d66';
        }
    }

    var lastSortingCriterion;
    var lastSortingCriterionOrder = 'desc';
    var lastBanSortingCriterion;
    var lastBanSortingCriterionOrder = 'desc';
    let userCriterions = ['users', 'followers', 'following', 'indexed-tweets', 'tweets'];
    let userBannedCriterions = ['banned-users', 'banned-followers', 'banned-date', 'banned-indexed-tweets', 'banned-motive'];
    function sortUsers(sortCriterion) {
        console.log('sortCriterion : ' + sortCriterion);

        // Unselecting all options.
        userCriterions.forEach(resetUserCriterion);

        loadFollowedUsers(sortCriterion);
    }
    function sortBannedUsers(sortCriterion) {
        console.log('sortCriterion : ' + sortCriterion);

        // Unselecting all options.
        userBannedCriterions.forEach(resetUserCriterion);

        loadBannedUsers(sortCriterion);
    }

    function resetUserCriterion(sortCriterion) {
        document.getElementById("sort-" + sortCriterion).src="/images/up-and-down-arrow.png";
    }

    function loadFollowedUsers(sortCriterion) {
        let sortCriterionOrder;
        if (lastSortingCriterion) {
            if (lastSortingCriterion == sortCriterion) {
                if (lastSortingCriterionOrder == 'desc') {
                    sortCriterionOrder = 'asc';
                } else {
                    sortCriterionOrder = 'desc';
                }
            } else {
                if (sortCriterion == 'users') {
                    sortCriterionOrder = 'asc';
                } else {
                    sortCriterionOrder = 'desc';
                }
            }
        } else {
            if (sortCriterion == 'users') {
                sortCriterionOrder = 'asc';
            } else {
                sortCriterionOrder = 'desc';
            }
        }
        lastSortingCriterion = sortCriterion;
        lastSortingCriterionOrder = sortCriterionOrder;
        let currentLanguage = '<%=$currentLanguage%>';
        let request = $.ajax({
            url: "/twitter_thought_police/twitter_followed_users",
            type: "POST",
            data: {
                sortCriterion      : sortCriterion,
                sortCriterionOrder : sortCriterionOrder,
                currentLanguage    : currentLanguage
            }
        });

        request.done(function(data) {
            $('#usersFollowed').html(data);

            // Display sorting by criterion.
            document.getElementById("sort-" + sortCriterion).src="/images/up-and-down-arrow-selected-" + sortCriterionOrder + ".png";
        });

        // Printing error message if case of failure
        request.fail(function(jqXHR, textStatus) {
            alert("Something went wrong : " + textStatus)
        });
    }

    function loadBannedUsers(sortCriterion) {
        console.log('sortCriterion : ' + sortCriterion);
        let sortCriterionOrder;
        if (lastBanSortingCriterion) {
            if (lastBanSortingCriterion == sortCriterion) {
                if (lastBanSortingCriterionOrder == 'desc') {
                    sortCriterionOrder = 'asc';
                } else {
                    sortCriterionOrder = 'desc';
                }
            } else {
                if (sortCriterion == 'users') {
                    sortCriterionOrder = 'asc';
                } else {
                    sortCriterionOrder = 'desc';
                }
            }
        } else {
            if (sortCriterion == 'users') {
                sortCriterionOrder = 'asc';
            } else {
                sortCriterionOrder = 'desc';
            }
        }
        lastBanSortingCriterion = sortCriterion;
        lastBanSortingCriterionOrder = sortCriterionOrder;
        let currentLanguage = '<%=$currentLanguage%>';
        let request = $.ajax({
            url: "/twitter_thought_police/twitter_banned_users",
            type: "POST",
            data: {
                sortCriterion      : sortCriterion,
                sortCriterionOrder : sortCriterionOrder,
                currentLanguage    : currentLanguage
            }
        });

        request.done(function(data) {
            $('#usersBanned').html(data);

            // Display sorting by criterion.
            document.getElementById("sort-" + sortCriterion).src="/images/up-and-down-arrow-selected-" + sortCriterionOrder + ".png";
        });

        // Printing error message if case of failure
        request.fail(function(jqXHR, textStatus) {
            alert("Something went wrong : " + textStatus)
        });
    }

    function openUserTweets(twitterUserName, twitterId) {
        // console.log('twitterId : ' + twitterId);
        // alert('pause');
        let currentLanguage = '<%=$currentLanguage%>';
        window.location.href = "/twitter_thought_police/open_user_tweets?twitterId=" + twitterId + '&twitterUserName=' + twitterUserName + '&currentLanguage=' + currentLanguage;
    }
</script>