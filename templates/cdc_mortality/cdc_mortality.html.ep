<script type="text/javascript">
	let currentGlobalTab;
	let currentStateTab;
	let currentTab;
	let tabs       = ['TOTALS', 'MODERNA', 'PFIZER-BIONTECH', 'JANSSEN'];
	let globalTabs = ['global-analysis', 'global-data'];
	let stateTabs  = ['state-TOTALS', 'state-MODERNA', 'state-PFIZER-BIONTECH', 'state-JANSSEN'];
	function openDelayTab(tabLabel) {
		currentTab = tabLabel;
		tabs.forEach(setTabButton);
		let tab = document.getElementById(tabLabel);
		let tabButton = document.getElementById(tabLabel + '-button');
		tab.style.display = 'block';
		tabButton.style.background = '#262436';
	}

	function openStateTab(tabLabel) {
		currentStateTab = tabLabel;
		stateTabs.forEach(setStateTabButton);
		let tab = document.getElementById(tabLabel);
		let tabButton = document.getElementById(tabLabel + '-button');
		tab.style.display = 'block';
		tabButton.style.background = '#262436';
	}

	function setStateTabButton(tabLabel) {
		if (tabLabel != currentTab) {
			let tab = document.getElementById(tabLabel);
			let tabButton = document.getElementById(tabLabel + '-button');
			tab.style.display = 'none';
			tabButton.style.background = '#4f4d66';
		}
	}

	function setTabButton(tabLabel) {
		if (tabLabel != currentTab) {
			let tab = document.getElementById(tabLabel);
			let tabButton = document.getElementById(tabLabel + '-button');
			tab.style.display = 'none';
			tabButton.style.background = '#4f4d66';
		}
	}

	function openGlobalTab(tabLabel) {
		currentGlobalTab = tabLabel;
		globalTabs.forEach(setGlobalTabButton);
		let tab = document.getElementById(tabLabel);
		let tabButton = document.getElementById(tabLabel + '-button');
		tab.style.display = 'block';
		tabButton.style.background = '#262436';
	}

	function setGlobalTabButton(tabLabel) {
		if (tabLabel != currentGlobalTab) {
			let tab = document.getElementById(tabLabel);
			let tabButton = document.getElementById(tabLabel + '-button');
			tab.style.display = 'none';
			tabButton.style.background = '#4f4d66';
		}
	}

	function openSymptoms(vaersSymptomType) {
		let modal = document.getElementById('fullScreenModal');
		modal.style.display     = 'block';
		let modalLoader = document.getElementById("modalLoader");
		modalLoader.style.display  = "inline-block";
		let currentLanguage     = '<%=$currentLanguage%>';
		let request = $.ajax({
		url: "/studies/vaers_fertility/symptoms",
		type: "POST",
		data: {
			vaersSymptomType : vaersSymptomType,
			currentLanguage  : currentLanguage
			}
		});

		request.done(function(data) {
			modalLoader.style.display  = "none";
			$('#fullScreenModalInnerContent').html(data);
		});

		// Printing error message if case of failure
		request.fail(function(jqXHR, textStatus) {
			alert("Something went wrong : " + textStatus)
		});
	}

	function openReports(reportType, reportNum) {
		$('#fullScreenModalInnerContent').html('');
		let modal = document.getElementById('fullScreenModal');
		modal.style.display     = 'block';
		let currentLanguage     = '<%=$currentLanguage%>';
		let request = $.ajax({
		url: "/studies/vaers_fertility/display_reports",
		type: "POST",
		data: {
			reportType      : reportType,
			reportNum       : reportNum,
			currentLanguage : currentLanguage
			}
		});

		request.done(function(data) {
			$('#fullScreenModalInnerContent').html(data);
		});

		// Printing error message if case of failure
		request.fail(function(jqXHR, textStatus) {
			alert("Something went wrong : " + textStatus)
		});
	}

	var modal = document.getElementById("fullScreenModal");
	var mainLoader = document.getElementById("mainLoader");

	$( document ).ready(function() {

		// Attending to the loader.
		mainLoader.style.display  = "none";

		// Get the main modal

		// When the user clicks anywhere outside of the modal, close it
		window.onclick = function(event) {
			if (event.target == modal) {
				modal.style.display = "none";
			}
		}

		// Get the <span> element that closes the main modal
		var span = document.getElementsByClassName("full-screen-modal-close")[0];

		// When the user clicks on <span> (x), close the modal
		span.onclick = function() {
			modal.style.display = "none";
		}
	});
</script>