	    <div class="text_separator"></div>
	    <div class="text_separator"></div>
	    <div style="width:100%;">
	    	<div style="width:355px;display: block;margin: auto;">
	    		% for my $productName (sort keys %products) {
	    			% my $label = $products{$productName} // die;
				    <div style="width:100%;">
