% use Data::Printer;
% my %australianData  = %$australianData;
<div style="width:100%;height: 100%;max-width:1000px;display:block;margin:auto;">
	% for my $compdate (sort{$a <=> $b} keys %australianData) {
		% for my $reportData (@{$australianData{$compdate}}) {
			% my %reportData = %$reportData;
			% my $cdcReportInternalId = $reportData{'cdcReportInternalId'} // die;
			% my $sCode2 = $reportData{'sCode2'} // die;
			% my $cdcSexName = $reportData{'cdcSexName'} // die;
			% my $patientAge = $reportData{'patientAge'} // 'Unknown';
			% my $vaccinationDate = $reportData{'vaccinationDate'} || 'Unknown';
			% my $onsetDate = $reportData{'onsetDateFixed'} || $reportData{'onsetDate'} || 'Unknown';
			% my $aEDescription = $reportData{'aEDescription'} // die;
			% my $permanentDisability = $reportData{'permanentDisability'} // die;
			% my $hospitalized = $reportData{'hospitalized'} // die;
			% my $patientDied = $reportData{'patientDied'} // die;
			% my $lifeThreatning = $reportData{'lifeThreatning'} // die;
			<div style="width: 100%;min-height:800px;font-size: 13px;border:1px solid black;border-radius:15px;margin-top:25px;">
				<div style="width:100%;overflow-y: auto;">
					<div style="width:calc(100% - 30px);display: flex;flex-wrap: wrap;height: 50px;">
						<div style="width: calc(45% - 15px);text-align: left;padding-left: 15px;padding-top: 15px;"><b>ID VAERS :</b> <%=$cdcReportInternalId%></div>
						<div style="width: calc(55% - 15px);text-align: right;padding: 15px 0px;"><b>Age :</b> <%=$patientAge%>, <b>Sexe :</b> <%=$cdcSexName%></div>
					</div>
					<div style="width:calc(100% - 30px);text-align: left;padding: 15px 15px;">
						<b>State</b> : <%=$sCode2%>
					</div>
					% for my $vaccineData (@{$reportData{'vaccines'}}) {
						 % my $substanceShortenedName = %$vaccineData{'substanceShortenedName'} // die;
					<div style="width:calc(100% - 30px);text-align: left;padding: 15px 15px;">
						<b>Product :</b> <%=$substanceShortenedName%>
					</div>
					% }
					<div style="width:calc(100% - 30px);display: flex;flex-wrap: wrap;height: 50px;">
						<div style="width: calc(45% - 15px);text-align: left;padding-left: 15px;padding-top: 15px;"><b>Vaccination Date :</b> <%=$vaccinationDate%></div>
						<div style="width: calc(55% - 15px);text-align: right;padding: 15px 0px;"><b>Onset Date :</b> <%=$onsetDate%></div>
					</div>
					<div style="width: calc(100% - 100px);padding: 2px 50px;">
						<hr>
					</div>
					<div style="width:calc(100% - 30px);text-align: left;padding: 15px 15px;">
						<b>Outcomes for the patient :</b>
					</div>
					<div style="width:calc(100% - 30px);display: flex;flex-wrap: wrap;height: 50px;">
						% if ($patientDied == 1) {
						<div style="width: calc(43% - 30px);text-align: left;padding-left: 15px;padding-top: 15px;"><b>Death :</b> Oui</div>
						% } else {
						<div style="width: calc(43% - 30px);text-align: left;padding-left: 15px;padding-top: 15px;"><b>Death :</b> Non</div>
						% }
						% if ($permanentDisability == 1) {
						<div style="width: calc(57% - 15px);text-align: left;padding: 15px 15px;"><b>Permanent Disability :</b> Oui</div>
						% } else {
						<div style="width: calc(57% - 15px);text-align: left;padding: 15px 15px;"><b>Permanent Disability :</b> Non</div>
						% }
					</div>
					<div style="width:calc(100% - 30px);display: flex;flex-wrap: wrap;height: 50px;">
						% if ($lifeThreatning == 1) {
						<div style="width: calc(43% - 30px);text-align: left;padding-left: 15px;padding-top: 15px;"><b>Life Threatning :</b> Oui</div>
						% } else {
						<div style="width: calc(43% - 30px);text-align: left;padding-left: 15px;padding-top: 15px;"><b>Life Threatning :</b> Non</div>
						% }
						% if ($hospitalized == 1) {
						<div style="width: calc(57% - 15px);text-align: left;padding: 15px 15px;"><b>Hospitalization :</b> Oui</div>
						% } else {
						<div style="width: calc(57% - 15px);text-align: left;padding: 15px 15px;"><b>Hospitalization :</b> Non</div>
						% }
					</div>
					<div style="width: calc(100% - 100px);padding: 2px 50px;">
						<hr>
					</div>
					<div style="width: calc(100% - 100px);padding: 2px 50px;">
						<hr>
					</div>
					<div style="width:calc(100% - 30px);text-align: left;padding: 15px 15px;">
						<b>Description :</b>
					</div>
					<div style="width: calc(100% - 30px);text-align: left;padding: 13px 15px;overflow-y: auto;">
						<%=$aEDescription%>
					</div>
					<div style="width: calc(100% - 100px);padding: 2px 50px;">
						<hr>
					</div>
					<div style="width:calc(100% - 30px);text-align: left;padding: 15px 15px;">
						<b>Sympt√¥mes :</b>
					</div>
					<div style="width: calc(100% - 30px);text-align: left;padding: 13px 15px;overflow-y: auto;">
					% my $symptoms;
					% for my $symptomName (sort keys %{$reportData{'symptoms'}}) {
						% $symptoms .= ", $symptomName" if $symptoms;
						% $symptoms .= $symptomName if !$symptoms;
					% }
					<%=$symptoms%>
					</div>
				</div>
			</div>
		% }
	% }
</div>