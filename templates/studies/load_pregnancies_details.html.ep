    <div class="report-confirmation-button" onclick="setReportAttribute('<%=$reportId%>', '<%=$sqlValue%>', 1)">
        Confirmer les d√©tails affich√©s
    </div>
    <div class="text_separator"></div>
    <div class="text_separator"></div>
    % } else {
        % die "something else to code";
    % }
