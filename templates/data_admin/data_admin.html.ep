        <div class="text_separator"></div>
        <a href="/data_admin/wizards/patient_ages_custom_export?currentLanguage=<%=$currentLanguage%>">&#10230; Générer un export personnalisé</a>
        <div class="text_separator"></div>
        % } else {

        % }
